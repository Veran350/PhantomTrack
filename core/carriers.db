# Run this once to create the DB  
import sqlite3  
conn = sqlite3.connect('core/carriers.db')  
c = conn.cursor()  
c.execute('''CREATE TABLE carriers  
             (prefix TEXT, carrier TEXT)''')  

data = [  
    ('703', 'MTN'), ('706', 'MTN'), ('803', 'MTN'),  
    ('802', 'Airtel'), ('808', 'Airtel'),  
    ('805', 'Glo'), ('807', 'Glo'),  
    ('809', '9mobile'), ('817', '9mobile')  
]  

c.executemany('INSERT INTO carriers VALUES (?,?)', data)  
conn.commit()  
conn.close()  
